@model SAP_Project.Models.PresentModels
@{
    ViewBag.Title = "GetStudentCource";
}

<h2>Students</h2>

<table class="table">
    <tr>
        <th>
            Student Name
        </th>
        <th>
            Courses Name
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Students)
    {
        <tr>
             <td>
                @Html.DisplayFor(modelItem => item.StudentName)
             </td>
            
            @foreach (var course in item.StudentCourses)
            {
               
                <td>
                @Html.DisplayFor(modelItem => course.Course.CourseName)
                </td>
            }
            
        </tr>
    }
</table>
