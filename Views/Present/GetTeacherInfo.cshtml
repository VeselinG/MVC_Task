@model SAP_Project.Models.PresentModels
@{
    ViewBag.Title = "GetTeacherInfo";
}

<h2>Teaacher Info</h2>

<table class="table">
    <tr>
        <th>
            Teacher Name
        </th>
        <th>
            Cources
        </th>        
        <th></th>
    </tr>

    @foreach (var item in Model.Teachers)
    {

        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TeacherName)
            </td>

            @foreach (var course in item.Courses)
            {
               
                <td>
                @Html.DisplayFor(modelItem => course.CourseName)
                </td>
            }

        </tr>
    }

    </table>

