@model SAP_Project.Models.PresentModels
@{
    ViewBag.Title = "GetAll";
}

<h2>Teachers</h2>

<table class="table">
    <tr>
        <th>
            Teacher Name
        </th>
        <th>
            Teacher Courses
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Teachers)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TeacherName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Courses.Count)
            </td>
        </tr>
    }
</table>

<h2>Students</h2>
<table class="table">
    <tr>
        <th>
            Student Name
        </th>
        <th>
           Student Course Year
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model.Students)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.StudentName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StudentCourseYear)
            </td>
        </tr>
    }

</table>